<section class="hero">
	<div class="container">
		<div class="hero__wrapper box-panel">
			<h1 class="hero__title">
				{{ heroTitle }}
			</h1>
			<div class="hero__text">
				<p>
					{{ heroText }}
				</p>
			</div>
			<a href="exchange.html" class="b-btn b-btn--primary">
				{{ heroBtn }}
			</a>
			<div class="hero-img">
				<div class="hero-img__item">
					<picture>
