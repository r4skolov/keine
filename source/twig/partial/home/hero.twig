						        srcset="images/home/hero/hero.webp 1x, images/home/hero/hero@2x.webp 1.3x"
						        type="image/webp">
						<source media="(min-width: 1024px)"
						        srcset="images/home/hero/hero.png 1x, images/home/hero/hero@2x.png 1.3x"
						        type="image/png">
						<source srcset="images/home/hero/hero-mob.webp 1x, images/home/hero/hero-mob@2x.webp 1.3x"
						        type="image/webp">
						<source srcset="images/home/hero/hero-mob.png 1x, images/home/hero/hero-mob@2x.png 1.3x"
						        type="image/png">
						<img src="images/home/hero/hero-mob.png" alt="" loading="lazy">
					</picture>
				</div>
